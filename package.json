{
  "name": "ya-music-controls",
  "version": "1.9.0",
  "description": "Ya.Music Controls plugin let you quickly see whatâ€™s currently playing, change a song, and play or pause music without changing a tab.",
  "main": "index.js",
  "repository": "https://github.com/vrizo/ya-music-controls.git",
  "author": "<kb@kernel-it.ru>",
  "license": "GPL-3.0-only",
  "private": false,
  "dependencies": {
    "eslint": "^7.17.0"
  },
  "scripts": {
    "test": "eslint ."
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config",
    "plugins": [
      "prettierx"
    ],
    "globals": {
      "chrome": true,
      "browser": true
    },
    "rules": {
      "security/detect-non-literal-regexp": "off",
      "security/detect-unsafe-regex": "off",
      "node/no-unsupported-features": "off",
      "multiline-ternary": [
        "error",
        "always-multiline"
      ],
      "prefer-const": "off",
      "valid-jsdoc": "off",
      "func-style": [
        2,
        "expression",
        {
          "allowArrowFunctions": true
        }
      ]
    }
  },
  "devDependencies": {
    "@logux/eslint-config": "^44.0.2",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-es5": "^1.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.1.0",
    "eslint-plugin-prettierx": "^0.14.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^26.0.1"
  }
